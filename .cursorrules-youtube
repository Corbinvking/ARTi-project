export const SERVICE_TYPE_LABELS = {
  [SERVICE_TYPES.DEDICATED]: 'Dedicated Video',
  [SERVICE_TYPES.NON_DEDICATED]: 'Non-Dedicated Video',
  [SERVICE_TYPES.COMMUNITY_POST]: 'Community Post',
